#!/bin/bash

set -e

# Not really using these yet:
PROJECT=$1

sudo apt-get install -y emacs24-nox curl tree unzip zip mosh zsh

cd $(dirname $0)

# Copy over resources
cp .zshrc ~
cp .gitconfig ~
cp .tmux.conf ~

# Copy over my .emacs.d
sudo -iu syme git clone git://github.com/abizern/prelude.git ~/.emacs.d
